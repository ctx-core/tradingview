{
	"name": "@ctx-core/tradingview",
	"version": "8.1.1",
	"description": "ctx-core tradingview svelte library",
	"keywords": [
		"ctx-core",
		"tradingview",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/tradingview#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/tradingview/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/tradingview/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"scripts": {
		"build": "npm run clean && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"devDependencies": {
		"@ctx-core/array": "workspace:^22.3.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.24",
		"@ctx-core/dom": "workspace:^10.0.27",
		"@ctx-core/object": "workspace:^20.5.0",
		"@ctx-core/uri": "workspace:^11.0.18",
		"@ctx-core/uuid": "workspace:^7.0.10",
		"svelte": "3.38.3",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}