{
	"name": "@ctx-core/tradingview",
	"version": "9.2.56",
	"description": "ctx-core tradingview svelte library",
	"keywords": [
		"ctx-core",
		"tradingview",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/tradingview#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/tradingview/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/tradingview.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Chart_tradingview",
		"./MarketOverview_tradingview",
		"./MediumWidget_tradingview",
		"./MiniSymbolOverview_tradingview"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.1.10",
		"@ctx-core/dom": "^15.1.53",
		"@ctx-core/object": "^31.1.30",
		"@ctx-core/uri": "^12.6.0",
		"@ctx-core/uuid": "^8.2.49",
		"svelte": "^4.2.8"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
