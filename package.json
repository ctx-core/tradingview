{
	"name": "@ctx-core/tradingview",
	"version": "9.0.68",
	"description": "ctx-core tradingview svelte library",
	"keywords": [
		"ctx-core",
		"tradingview",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/tradingview#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/tradingview/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/tradingview/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@ctx-core/array": "workspace:^23.0.23",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.77",
		"@ctx-core/dom": "workspace:^11.0.30",
		"@ctx-core/object": "workspace:^21.0.22",
		"@ctx-core/uri": "workspace:^12.0.16",
		"@ctx-core/uuid": "workspace:^8.0.15",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"svelte": "3.44.1",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
